<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="StateButton">
        <!--圆角半径-->
        <attr name="corner_radius" format="dimension" />
        <!--边框颜色-->
        <attr name="border_color" format="color" />
